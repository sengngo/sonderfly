<nav>
  <div id="logo">
    <%= link_to image_tag("sonderfly_logo_beta.png", size: '185x52'), root_path %>
  </div>
  <% if user_signed_in? %>
    <%= link_to current_user.full_name, edit_user_registration_path %>
      <%= link_to "Explore", posts_path %>
      
      <div class="float-right">
        <%= link_to "New Post", new_post_path, class: "button"%> 
        <%= link_to "Sign Out", destroy_user_session_path,  class: "button", method: :delete %>
      </div>
  <% else %>
    <%= link_to "Explore", posts_path %>
      <div class="float-right">
      <%= link_to "Join", new_user_session_path %> |
      <%= link_to "Sign Up", new_user_registration_path, class: 'button' %>   
      </div>   
  <% end %>
</nav>